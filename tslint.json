{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": false,
      "variable-name": [true, "allow-leading-underscore"],
      "curly": false,
      "no-console": false,
      "max-classes-per-file": false,
      "object-literal-shorthand": false,
      "ordered-imports": [
        true,
        {
          "named-imports-order": "lowercase-last"
        }
      ]
    },
    "rulesDirectory": []
}